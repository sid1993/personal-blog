<%- include('partials/header'); -%>
<h1>Compose</h1>

<form action="/compose" method="post">
	<div class="form-group">
		<label>Title</label>
		<input type="text" class="form-control" name="composerTitle" />
	</div>
	<div class="form-group">
		<label>Post</label>
		<textarea class="form-control" name="composerPost"></textarea>
	</div>
	<button type="submit" class="btn btn-lg btn-info">Publish</button>
</form>
<%- include('partials/footer'); -%>
